</bean>

<bean class="kosta.mvc.domain.Board" id="board3">
	<property name="content" value="집가고 싶다 줴엔장"/>
	<property name="date" value="2020-05-12"/>
	<property name="no" value="1"/>
	<property name="subject" value="그제"/>
